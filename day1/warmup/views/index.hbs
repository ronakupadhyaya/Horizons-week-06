<h1>Some random movies</h1>

<table class="table table-striped table-bordered">
  {{#each data}}
    <tr>
      {{#if @index}} 
      {{#each this}}
      <td>{{this}}</td>
      {{/each}}

    {{else}}
      {{#each this}}
    <th>{{this}}</th>
    {{/each}}
    {{/if}}
    </tr>
  {{/each}}

 

</table>
 